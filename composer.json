{
    "name": "telegram-video-bot/compressor",
    "type": "project",
    "description": "Telegram Video Compression Bot",
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "files": [
            "config.php",
            "functions.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": []
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
